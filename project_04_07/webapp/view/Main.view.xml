<mvc:View controllerName="project0407.controller.Main"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"   xmlns:t = "sap.ui.table"   >
    <Page id="page" title="{i18n>title}">
       <VBox>
       
       <Input value = "{/inpValue}" id = "idInput" />    <!--World-->
        <Button text = "λ¨λΈ μ„Έν…" press = "onSetData" />
		<Text text = "Hello {test>/textValue}" />      <!--Hello-->
		
        </VBox>

    <t:Table
				rows="{test>/datas}"
				selectionMode="MultiToggle"
				paste="onPaste"
				ariaLabelledBy="title">
				<t:extension>
					<OverflowToolbar style="Clear">
						<Title id="title" text="Products"/>
					</OverflowToolbar>
				</t:extension>
				<t:columns>
					<t:Column width="11rem">
						<Label text="Name" />
						<t:template>
							<Text text="{test>name}" wrapping="false" />
						</t:template>
					</t:Column>

					<t:Column width="11rem">
						<Label text="age" />
						<t:template>
							<Text text="{test>age}" wrapping="false" />
						</t:template>
					</t:Column>

					<t:Column width="18rem">
						<Label text="tel" />
						<t:template>
							<Text text="{test>tel}" wrapping="false" />
						</t:template>
					</t:Column>
					
				</t:columns>
			</t:Table>


    </Page>
</mvc:View>
